{
  "name": "swapi-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "local": "nest build  --tsc && sls offline",
    "deploy": "nest build  --tsc && sls deploy",
    "build": "nest build  --tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/fn-people/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/fn-people/test/jest-e2e.json"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.15.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.3",
    "@types/aws-lambda": "^8.10.145",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lambda-tester": "^4.0.3",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dynamodb-admin": "^4.6.1",
    "esbuild": "^0.14.31",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "lambda-tester": "^4.0.1",
    "prettier": "^3.0.0",
    "serverless": "^3.11.0",
    "serverless-dynamodb": "^0.2.54",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.52.1",
    "serverless-jetpack": "^0.3.3",
    "serverless-offline": "^13.8.0",
    "serverless-plugin-typescript": "^2.1.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ]
  }
}
